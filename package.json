    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "pre-commit-cmd": "yarn lint:fix && yarn jest",
    "screenshot:demo": "yarn start && yarn test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storyshots": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/graphql": "^14.2.0",
    "@types/puppeteer": "^1.12.3",
    "@types/react-router-config": "^1.1.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.13",
    "@types/webpack": "^4.4.27",
    "@types/yup": "^0.26.12",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "formik": "^1.5.2",
    "graphql": "^14.2.1",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "jest-puppeteer": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "puppeteer": "^1.14.0",
    "rbx": "^2.1.0",
    "react-bulma-components": "^2.3.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4",
    "styled-components": "^4.2.0",
    "styled-reset": "^2.0.6",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "webpack-merge": "^4.2.1",
    "yup": "^0.27.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "test": {
        "plugins": [
          "require-context-hook"
        ]
      }
    }
  },
  "jest": {
    "globalSetup": "<rootDir>/puppeteer/setup.js",
    "globalTeardown": "<rootDir>/puppeteer/teardown.js",
    "preset": "jest-puppeteer",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/setupTests.tsx",
      "<rootDir>/config/jest/registerContext.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "<rootDir>/puppeteer/environment.js",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Volumes/workspace/tmp/my-app/node_modules/jest-watch-typeahead/filename.js",
      "/Volumes/workspace/tmp/my-app/node_modules/jest-watch-typeahead/testname.js"
    ]
  }
}
